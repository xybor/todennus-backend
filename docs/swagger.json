                    "400": {
                        "description": "Bad request",
                        "schema": {
                            "$ref": "#/definitions/standard.SwaggerBadRequestErrorResponse"
                        }
                    },
                    "401": {
                        "description": "Invalid credentials",
                        "schema": {
                            "$ref": "#/definitions/standard.SwaggerInvalidCredentialsErrorResponse"
                        }
                    }
                }
            }
        },
        "/users/{user_id}": {
            "get": {
                "description": "Get an user information by user id. \u003cbr\u003e\nRequire scope `read:user.role` to get role of user.",
